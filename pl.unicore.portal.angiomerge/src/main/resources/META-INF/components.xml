<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config/>
	<bean scope="prototype" class="pl.edu.icm.unicore.portal.angio.ui.AngioMergeMainView"/>
	<bean class="pl.edu.icm.unicore.portal.angio.ui.AngioMergeNavFactory"/>
		
	<bean class="eu.unicore.portal.core.i18n.MessageSourceBundles">
		<property name="bundle" value="pl/edu/icm/unicore/portal/angio/i18n/AngioMessages" />
	</bean>

</beans>